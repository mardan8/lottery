.prizeState{
height:50rpx;
border:2rpx solid #ddd;
padding:25rpx 0rpx 25rpx 26rpx;
}
.choose{
display:inline-block;
width:212rpx;
background-color:#999999;
height:50rpx;
border-radius:15rpx;
color:#fff;
text-align:center;
margin-right: 26rpx;
}
.checked{
  background-color: #cc3333;
}
.orderTop{
height:70rpx;
line-height:70rpx;
padding-left:30rpx;
position:relative;
background-color: #999999;
}
.orderTop text{
  margin-right: 50rpx;
  font-size:34rpx;
}
.orderBottom{
  border-bottom:2rpx solid #999999;
}
.orderBottom text{
display:block;
padding-left:30rpx;
height:60rpx;
line-height:60rpx;
}
.bottomRight image{
  width:55rpx;
  height:55rpx;
  position: absolute;
  top: -65rpx;
}
.bottomLeft{
  display: inline-block;
  width: 680rpx;
}
.bottomRight{
  display: inline-block;
  position: relative;
}
.orderTop image{
  width: 55rpx;
  height: 55rpx;
  position:absolute;
  top:10rpx;
  right:16rpx;
}
.noOrder{
height:200rpx;
text-align:center;
padding-top:50rpx;
}
.order{
  border-bottom: 2rpx solid #fff;
}