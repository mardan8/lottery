.prizeState{
height:50rpx;
border:2rpx solid #ddd;
padding:15rpx 0rpx 15rpx 26rpx;
text-align:center;
}
.choose{
display:inline-block;
width:272rpx;
background-color:#999999;
height:50rpx;
border-radius:15rpx;
color:#fff;
text-align:center;
margin-right: 42rpx;
}
.checked{
  background-color: #cc3333;
}
.chupiaoBox{
height:80rpx;
line-height:80rpx;
border-bottom:2rpx solid #dadada
}
.betnumber{
float:left;
width:60%;
padding-left:20rpx
}
.chupiao,.yichupiao{
float:right;
width:120rpx;
height:60rpx;
background-color:#cc3333;
border-radius:10rpx;
margin-right:20rpx;
margin-top:10rpx;
line-height:60rpx;
font-size:32rpx;
text-align:center;
color:#fff;
}
.yichupiao{
  background-color:#999999;
}
.tip{
  text-align:center;
  line-height:200rpx;
}
.uploader-container {
    width: 100%;
    height: 400rpx;
    padding: 20rpx 20rpx 20rpx 0;
    display: flex;
    align-content: center;
    justify-content: center;
    box-sizing: border-box;
    border-top: 1px solid rgba(0, 0, 0, .1);
    margin-bottom:20rpx;
}
.uploader-image {
    width: 100%;
    height:400rpx;
}